{
  "name": "md5html",
  "version": "1.0.2",
  "description": "Offline MD5 Calculator",
  "scripts": {
    "dev": "cp node_modules/crypto-js/crypto-js.js src/ && elm-live src/Main.elm --open --pushstate --dir=./src --start-page=debug.html -- --debug --output=src/elm.js",
    "clean": "rm -rf dist/ node_modules/ elm-stuff/ elm.js",
    "debug": "elm-live src/Main.elm -- --output elm.js",
    "start": "npm run dist && serve dist",
    "bundle": "uglifyjs elm.js src/main.js -o dist/bundle.js --define DEBUG=false -c -m",
    "build": "elm make src/Main.elm --output elm.js",
    "workbox": "workbox generateSW",
    "copy": "cp node_modules/crypto-js/crypto-js.js src/worker.js src/main.css *.webmanifest *.ico *.png *sw*.js src/index.html ./dist",
    "dist": "rm -rf dist && mkdir dist && npm run build && npm run bundle && npm run copy && npm run workbox",
    "test": "elm make src/Main.elm --output elm.js && standard src/main.js src/worker.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/md5html.git"
  },
  "author": "tosh satake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/md5html/issues"
  },
  "homepage": "https://github.com/tshm/md5html",
  "devDependencies": {
    "crypto-js": "^3.1.9-1",
    "elm": "^0.19.0-bugfix2",
    "jshashes": "^1.0.6",
    "lighthouse": "^4.0.0-alpha.2-3.2.1",
    "serve": "^10.1.1",
    "standard": "^12.0.1",
    "uglify-js": "^3.4.9",
    "workbox-cli": "^3.6.3"
  },
  "dependencies": {
    "elm-live": "^3.2.3"
  }
}
