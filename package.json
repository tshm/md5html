{
  "name": "md5html",
  "version": "1.0.2",
  "description": "Offline MD5 Calculator",
  "scripts": {
    "devel": "cp node_modules/crypto-js/crypto-js.js src/ && elm-live --warn src/Main.elm --output src/elm.js",
    "clean": "rm -rf dist/ node_modules/ elm-stuff/ elm.js",
    "debug": "elm-live --debug --warn --yes src/Main.elm --output elm.js",
    "start": "npm run dist && serve dist",
    "install": "elm-package install --yes",
    "bundle": "uglifyjs elm.js src/main.js -o dist/bundle.js --define DEBUG=false -c -m",
    "build": "elm-make --warn --yes src/Main.elm --output elm.js",
    "workbox": "workbox-cli generate:sw",
    "copy": "cp node_modules/jshashes/hashes.min.js src/main.css manifest.* *.ico *.png *sw*.js index.html ./dist",
    "dist": "rm -rf dist && mkdir dist && npm run build && npm run bundle && npm run copy && npm run workbox",
    "analyse": "elm-analyse",
    "test": "elm-make --warn --yes src/Main.elm --output elm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/md5html.git"
  },
  "author": "tosh satake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/md5html/issues"
  },
  "homepage": "https://github.com/tshm/md5html",
  "devDependencies": {
    "crypto-js": "^3.1.9-1",
    "elm": "^0.18.0",
    "elm-analyse": "^0.9.3",
    "jshashes": "^1.0.6",
    "lighthouse": "^2.2.1",
    "serve": "^6.0.1",
    "uglify-js": "^3.0.11",
    "workbox-cli": "^1.0.1"
  }
}
