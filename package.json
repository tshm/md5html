{
  "name": "md5html",
  "version": "1.0.3",
  "description": "Offline MD5 Calculator",
  "scripts": {
    "dev": "parcel serve src/debug.html",
    "start": "parcel serve src/index.html",
    "clean": "rm -rf dist/ node_modules/ elm-stuff/",
    "build": "parcel build --public-url . src/index.html",
    "deploy": "gh-pages -d dist",
    "test": "yarn lint",
    "lint": "eslint src/main.js src/worker.js"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/md5html.git"
  },
  "author": "tosh satake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/md5html/issues"
  },
  "homepage": "https://github.com/tshm/md5html",
  "devDependencies": {
    "@parcel/packager-raw-url": "2.6.0",
    "@parcel/transformer-elm": "2.6.0",
    "@parcel/transformer-webmanifest": "2.6.0",
    "elm": "^0.19.0-bugfix6",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "gh-pages": "^4.0.0",
    "node-elm-compiler": "^5.0.1",
    "parcel": "^2.6.0"
  },
  "dependencies": {
    "crypto-js": "^4.1.1"
  }
}
